{
  "name": "Boxee Remote",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A remote control for the Boxee Box",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["http://*/*" ],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}
